<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:views="clr-namespace:Kakao1.Forms.Views"
    xmlns:core="clr-namespace:Kakao1.Core.ViewServices;assembly=Kakao1.Core"
    xmlns:Test="clr-namespace:Kakao1.LayoutSupport.UI.Units;assembly=Kakao1.LayoutSupport"
    xmlns:regionName="clr-namespace:Kakao1.Core.Names;assembly=Kakao1.Core">
    <Style TargetType="{x:Type views:Kakao1Window}">
        <Setter Property="SizeToContent" Value="WidthAndHeight"/>
        <Setter Property="Background" Value="#FFFFFF"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type views:Kakao1Window}">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <core:PrismRegion RegionName='{x:Static regionName:RegionNameManager.MainRegion}'/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
